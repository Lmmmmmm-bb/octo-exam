{
  "name": "octo-exam",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "packageManager": "pnpm@7.33.3",
  "scripts": {
    "dev": "vite & npm run style:watch",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint:check": "npx prettier --check .",
    "lint:format": "npx prettier --write .",
    "lint:fix": "eslint --ext .tsx,.ts,.js,.vue --fix ./src --fix",
    "commit": "node ./node_modules/cz-customizable/standalone.js",
    "prepare": "husky install",
    "style:watch": "./node_modules/.bin/typed-scss-modules src --watch --ignore \"**/index.scss\" \"**/styles/*.scss\""
  },
  "dependencies": {
    "@vueuse/motion": "^2.0.0",
    "axios": "^1.4.0",
    "crypto-ts": "^1.0.2",
    "element-plus": "^2.3.7",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-i18n": "9.2.2",
    "vue-router": "4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@element-plus/icons-vue": "^2.1.0",
    "@types/node": "^18.16.19",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^7.0.0",
    "driver.js": "^0.9.8",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.24",
    "prettier": "2.8.8",
    "sass": "^1.63.6",
    "tailwindcss": "^3.3.2",
    "typed-scss-modules": "^7.1.1",
    "typescript": "~5.1.6",
    "unplugin-element-plus": "^0.7.1",
    "unplugin-icons": "^0.16.3",
    "vite": "^3.2.7",
    "vue-tsc": "^1.8.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,ts,tsx,vue}": "eslint --ext .tsx,.ts,.js,.vue --fix ./src"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  }
}
